<UserControl x:Class="DaqProto.MonitorTool.View.NumericalListViewers.NumericalListViewerPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DaqProto.MonitorTool.View.NumericalListViewers"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="400" Loaded="NumericalListViewerPanel_Loaded" SizeChanged="NumericalListViewerPanel_SizeChanged">
    <DockPanel LastChildFill="True" Background="{x:Static SystemColors.ControlDarkBrush}">
        <StackPanel DockPanel.Dock="Right" Orientation="Vertical" Background="{x:Static SystemColors.ControlBrush}">
            <ToggleButton Content="現" FontWeight="Bold" Height="25" Width="25" />
        </StackPanel>
        <StackPanel x:Name="ParentPanel" DockPanel.Dock="Left" Orientation="Vertical" />
    </DockPanel>
</UserControl>
